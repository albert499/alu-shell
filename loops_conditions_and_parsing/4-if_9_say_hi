#!/usr/bin/env bash
# Initialize counter
counter=0

# Loop until counter is 10
while [ $counter -lt 10 ]; do
    # Check if it's the 9th iteration
    if [ $counter -eq 8 ]; then
        echo "Best School"
        echo "Hi"
    else
        echo "Best School"
    fi
    # Increment counter
    ((counter++))
done
